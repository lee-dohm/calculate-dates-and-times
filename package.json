{
  "name": "calculate-dates-and-times",
  "version": "0.0.0",
  "private": true,
  "description": "Generate a date or time to be used in a later Action ",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx ncc build ./src/main.ts",
    "ci": "npm run format-check && npm test",
    "format": "prettier --write **/*.ts **/*.md **/*.yaml **/*.yml",
    "format-check": "prettier --check **/*.ts **/*.md **/*.yaml **/*.yml",
    "start": "npx ncc run ./src/main.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lee-dohm/calculate-dates-and-times.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "lee-dohm",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "moment": "^2.28.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.10.1",
    "@vercel/ncc": "^0.24.1",
    "jest": "^24.9.0",
    "jest-circus": "^26.4.2",
    "js-yaml": "^3.14.0",
    "prettier": "^2.1.2",
    "ts-jest": "^24.3.0",
    "typescript": "^4.0.2"
  }
}
